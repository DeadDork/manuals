h.sortResults_=function(a,b,c){for(var d=0;d<a.length;d++)for(var e=a[d],f=e.score=0;f<b.length;f++){var g=b[f],v=!1,l=e.title.toLowerCase().indexOf(g);if(-1<l){v=!0;e.score+=1;var F=0,F=c==r.search.suggest.Category.REFERENCE?l/e.title.length:(e.title.length-l)/e.title.length;e.score+=F;l=g.length/e.title.length;e.score+=l}for(l=0;l<e.keywords.length;l++)e.keywords[l].toLowerCase()==g&&(e.score+=.25,v=!0);v||(e.score-=1E3)}return a.filter(this.filterResult_).sort(this.compareResult_)};
h.compareResult_=function(a,b){return b.score-a.score};h.filterResult_=function(a){return 0<a.score};h.tokenize_=function(a){return a.split(/\s*\b\s*/)};
 })();

  if (window.jQuery) {
    $(document).ready(function() {
      if (window.devsite && window.devsite.search) {
        window.devsite.search.init('en')
      }
    });
  }
</script>

        
      
