p.uri.utils.haveSameDomain=function(a,c){var d=p.uri.utils.split(a),e=p.uri.utils.split(c);return d[p.uri.utils.ComponentIndex.DOMAIN]==e[p.uri.utils.ComponentIndex.DOMAIN]&&d[p.uri.utils.ComponentIndex.SCHEME]==e[p.uri.utils.ComponentIndex.SCHEME]&&d[p.uri.utils.ComponentIndex.PORT]==e[p.uri.utils.ComponentIndex.PORT]};
p.uri.utils.assertNoFragmentsOrQueries_=function(a){if(p.DEBUG&&(0<=a.indexOf("#")||0<=a.indexOf("?")))throw Error("goog.uri.utils: Fragment or query identifiers are not supported: ["+a+"]");};p.uri.utils.appendQueryData_=function(a){if(a[1]){var c=a[0],d=c.indexOf("#");0<=d&&(a.push(c.substr(d)),a[0]=c=c.substr(0,d));d=c.indexOf("?");0>d?a[1]="?":d==c.length-1&&(a[1]=void 0)}return a.join("")};
p.uri.utils.appendKeyValuePairs_=function(a,c,d){if(p.isArray(c)){p.asserts.assertArray(c);for(var e=0;e<c.length;e++)p.uri.utils.appendKeyValuePairs_(a,String(c[e]),d)}else null!=c&&d.push("&",a,""===c?"":"=",p.string.urlEncode(c))};p.uri.utils.buildQueryDataBuffer_=function(a,c,d){p.asserts.assert(0==Math.max(c.length-(d||0),0)%2,"goog.uri.utils: Key/value lists must be even in length.");for(d=d||0;d<c.length;d+=2)p.uri.utils.appendKeyValuePairs_(c[d],c[d+1],a);return a};
