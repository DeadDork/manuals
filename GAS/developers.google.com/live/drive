p.uri.utils.findParam_=function(a,c,d,e){for(var g=d.length;0<=(c=a.indexOf(d,c))&&c<e;){var h=a.charCodeAt(c-1);if(h==p.uri.utils.CharCode_.AMPERSAND||h==p.uri.utils.CharCode_.QUESTION)if(h=a.charCodeAt(c+g),!h||h==p.uri.utils.CharCode_.EQUAL||h==p.uri.utils.CharCode_.AMPERSAND||h==p.uri.utils.CharCode_.HASH)return c;c+=g+1}return-1};p.uri.utils.hashOrEndRe_=/#|$/;p.uri.utils.hasParam=function(a,c){return 0<=p.uri.utils.findParam_(a,0,c,a.search(p.uri.utils.hashOrEndRe_))};
p.uri.utils.getParamValue=function(a,c){var d=a.search(p.uri.utils.hashOrEndRe_),e=p.uri.utils.findParam_(a,0,c,d);if(0>e)return null;var g=a.indexOf("&",e);if(0>g||g>d)g=d;e+=c.length+1;return p.string.urlDecode(a.substr(e,g-e))};p.uri.utils.getParamValues=function(a,c){for(var d=a.search(p.uri.utils.hashOrEndRe_),e=0,g,h=[];0<=(g=p.uri.utils.findParam_(a,e,c,d));){e=a.indexOf("&",g);if(0>e||e>d)e=d;g+=c.length+1;h.push(p.string.urlDecode(a.substr(g,e-g)))}return h};
p.uri.utils.trailingQueryPunctuationRe_=/[?&]($|#)/;p.uri.utils.removeParam=function(a,c){for(var d=a.search(p.uri.utils.hashOrEndRe_),e=0,g,h=[];0<=(g=p.uri.utils.findParam_(a,e,c,d));)h.push(a.substring(e,g)),e=Math.min(a.indexOf("&",g)+1||d,d);h.push(a.substr(e));return h.join("").replace(p.uri.utils.trailingQueryPunctuationRe_,"$1")};p.uri.utils.setParam=function(a,c,d){return p.uri.utils.appendParam(p.uri.utils.removeParam(a,c),c,d)};
p.uri.utils.appendPath=function(a,c){p.uri.utils.assertNoFragmentsOrQueries_(a);p.string.endsWith(a,"/")&&(a=a.substr(0,a.length-1));p.string.startsWith(c,"/")&&(c=c.substr(1));return p.string.buildString(a,"/",c)};
